{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"target": "es2021",
		"keepClassNames": true,
		"loose": false,
		"externalHelpers": false,
		"preserveAllComments": false,
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"],
			"@app/*": ["src/app/*"],
			"@business/*": ["src/business/*"],
			"@agent/*": ["src/agent/*"],
			"@utils/*": ["src/utils/*"],
			"@types/*": ["src/type/*"],
			"@prisma/*": ["./prisma/generated/*"]
		}
	},
	"module": {
		"type": "commonjs",
		"strict": false,
		"strictMode": false,
		"lazy": false,
		"noInterop": false,
		"ignoreDynamic": true //保留源码中的动态导入，以在cjs产物中支持引入纯esm包
	},
	"minify": false,
	"sourceMaps": true,
	"inlineSourcesContent": true
}
